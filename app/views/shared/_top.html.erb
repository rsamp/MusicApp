<%= link_to("HOME", users_url, method: :create) %>
<% if current_user %>
  <%= current_user.email %>
  <%= button_to("Log Out", session_url, method: :delete) %>
<% else %>
  <%= button_to("Log In", new_session_url, method: :get) %>
  <%= button_to("Sign Up", new_user_url, method: :get) %>
<% end %>
